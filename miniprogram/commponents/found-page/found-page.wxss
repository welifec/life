page {
  background-color: #F5F5F5;
  height: 100%
}

.rec-found {
  display: flex;
  flex-direction: column;
}

.rec-found-head {
  display: flex;
  flex-direction: row;
  height: 72rpx;
  background: #FFFFFF;
  justify-content: space-around;
  align-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  z-index: 999
}

.rec-found-head view {
  width: 250rpx;
  text-align: center;
}

.rec-found-head text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #989898;
}

.content {
  margin-top: 60rpx;
}

.rec-found-list {
  width: 690rpx;
  background: #FFFFFF;
  border-radius: 30rpx;
  margin: 30rpx 30rpx 24rpx 30rpx;
}

.rec-found-list-item {
  margin: 43rpx 0 0 36rpx;
  position: relative;
  z-index: 1;
}

.rec-found-list-item-text1 {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #146EF0;

}

.rec-found-list-item-text2 {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #222222;
}

.rec-found-list-item2 {
  width: 120rpx;
  height: 54rpx;
  background: #E0EDFF;
  border-radius: 27rpx 0rpx 0rpx 27rpx;
  text-align: center;
  position: absolute;
  z-index: 2;
  margin: 30rpx 0 0 570rpx;
}

.rec-found-list-item2 text {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #146EF0;
}

.rec-found-list-item3 {
  margin: 49rpx 0 43rpx 36rpx;
}

.rec-found-list-item3 view {
  margin: 0 0 31rpx 0;
}

.rec-found-list-item3 view text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #545454;
}

.rec-job {
  box-shadow: 0px 4px 10px 1px rgba(128, 115, 115, 0.200000);
  background-color: rgba(20, 110, 240, 1.000000);
  border-radius: 50%;
  height: 120rpx;
  width: 120rpx;
  display: flex;
  flex-direction: column;
  position: absolute;
  z-index: 2;
  margin: 926rpx 36rpx 72rpx 594rpx;

}
swiper-item {
  overflow: scroll;
}
.rec-job-item {
  width: 46rpx;
  height: 75rpx;
  display: flex;
  flex-direction: row;
  margin: 23rpx 0 0 37rpx;
}

.rec-job-ImageText {
  height: 75rpx;
  display: flex;
  flex-direction: column;
  width: 46rpx;
}

.rec-job-item-box {
  width: 46rpx;
  height: 75rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.rec-job-item-text {
  height: 23rpx;
  margin-top: 12rpx;
  display: flex;
  flex-direction: column;
  width: 46rpx;
}

.rec-job-item-text text {
  width: 46rpx;
  height: 23rpx;
  overflow-wrap: break-word;
  color: rgba(255, 255, 255, 1);
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  text-align: left;
  white-space: nowrap;
  line-height: 24rpx;
  display: block;
}

.rec-job-item-image {
  width: 42rpx;
  height: 40rpx;
  margin-left: 2rpx;
}

.nav-switch-style {
  width: 250rpx;
  height: 72rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  align-content: center;
}

.nav-switch-style view {
  display: flex;
  width: 48rpx;
  height: 6rpx;
  background: #146EF0;
  border-radius: 3rpx;
}

.nav-switch-style text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #222222;
}

.fabutton {
  display: flex;
  position: fixed;
  bottom: 0;
  margin: 0 36rpx 72rpx 594rpx;
}