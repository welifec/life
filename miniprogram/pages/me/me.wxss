page {
  background-color: #F5F5F5;
}

.me {
  width: 690rpx;
  height: 220rpx;
  background: #FFFFFF;
  border-radius: 18rpx;
  display: flex;
  flex-direction: column;
  margin: 30rpx 30rpx 24rpx 30rpx;
}

.me-if {
  display: flex;
  flex-direction: row;
  align-items: center;
}


.me-if image {
  width: 138rpx;
  height: 138rpx;
  margin: 41rpx 0 41rpx 24rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}

.me-if-text {
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: 800;
  color: #222222;
}

.me-else {
  display: flex;
  flex-direction: row;
}

.me-else-image {
  width: 138rpx;
  height: 138rpx;
  margin: 41rpx 0 41rpx 24rpx;
}

.me-else-view2 {
  margin: 80rpx 0 80rpx 24rpx;
}

.me-else-view2 button {
  width: 210rpx;
  height: 60rpx;
  background: #146EF0;
  border-radius: 30rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FEFFFE;
  display: flex;
  align-items: center;
  justify-content: center;
}

.me-model {
  margin-top: 24rpx;
  background-color: #FFFFFF;
  display: flex;
  flex-direction: column;
}

.me-model view {
  display: flex;
  align-items: center;
}


.me-model image {
  width: 38rpx;
  height: 38rpx;
  margin-right: 23rpx;
}

.me-model text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #222222;
}

.me-exit {
  margin-top: 100rpx;
}

.me-switchaccount {
  border-radius: 30rpx;
  background-color: #146EF0;
  color: #FFFFFF;
  margin-bottom: 25rpx;
}

.me-exit-bt {
  border-radius: 30rpx;
  background-color: #146EF0;
  color: #FFFFFF;

}