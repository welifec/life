}

.rec-job-list-box-text2 {
  margin-left: 18rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #222222;
  font-size: 30rpx;
}

.rec-job-list-box-text3 {
  position: absolute;
  margin: 42rpx 30rpx 243rpx 553rpx;
  font-family: PingFang SC;
  font-weight: 500;
  font-size: 30rpx;
  color: #E7615B;
  right: 30rpx;
}

.rec-job-list-box2 {
  display: flex;
  margin: 51rpx 305rpx 45rpx 35rpx;
}

.rec-job-list-box2-image1 {
  width: 28rpx;
  height: 28rpx;
  margin-right: 24rpx;
  margin-top: 6rpx;
}

.rec-job-list-box2-text1 {
  word-break: keep-all;
